{
  "name": "mission-sea-turtle-nest",
  "friendlyName": "Mission Sea Turtle Nest",
  "version": "0.8.1",
  "homepage": "https://turtle-quest.vercel.app/",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "eslint": "^9.39.2",
    "gh-pages": "^6.2.0",
    "prettier": "^3.7.4",
    "tailwindcss": "^4.1.17",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.50.1"
  },
  "scripts": {
    "dev": "npx vite",
    "start": "npm run dev",
    "build": "tsc -b && vite build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "create:prey": "node --experimental-strip-types create.ts prey",
    "create:obstacle": "node --experimental-strip-types create.ts obstacle",
    "lint": "eslint .",
    "format": "prettier . --write"
  },
  "dependencies": {
    "vite": "^5.4.10"
  }
}
