{
  "name": "mission-sea-turtle-nest",
  "friendlyName": "Mission Sea Turtle Nest",
  "version": "0.8.0",
  "homepage": "https://turtle-quest.vercel.app/",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "gh-pages": "^6.2.0",
    "tailwindcss": "^4.1.17",
    "ts-loader": "^9.5.1",
    "typedoc": "^0.25.13",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "dev": "npx vite",
    "start": "npm run dev",
    "docs": "npx typedoc src/characters/abstract/* src/characters/interfaces/* src/levels/* src/utils/* src/enums/* src/Background.ts src/checkTurtle.ts src/Game.ts --name TurtleQuest --out dev/docs/",
    "build": "tsc -b && vite build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "create:prey": "node --experimental-strip-types create.ts prey",
    "create:obstacle": "node --experimental-strip-types create.ts obstacle"
  },
  "dependencies": {
    "vite": "^5.4.10"
  }
}
