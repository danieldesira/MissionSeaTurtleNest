{
  "compilerOptions": {
    "jsx": "react-jsx",
    "sourceMap": true,
    "module": "ESNext",
    "target": "ES2022",
    "allowJs": true,
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noEmit": true,
    "lib": ["ES2022", "DOM"]
  },
  "include": ["src"],
  "exclude": ["src/serviceWorkers"]
}
